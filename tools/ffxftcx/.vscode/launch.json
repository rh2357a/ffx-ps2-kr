{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "run extract test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}\\build\\debug\\bin\\${workspaceFolderBasename}.exe",
      "args": ["-e", "ffxkr.iso", "test"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}\\build\\debug\\bin",
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "${env:MSYS2_HOME}\\mingw64\\bin\\gdb.exe",
      "preLaunchTask": "Build",
      "internalConsoleOptions": "neverOpen",
      "setupCommands": [
        {
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "text": "set print unicode on",
          "ignoreFailures": true
        },
        {
          "text": "set target-charset UTF-8",
          "ignoreFailures": true
        },
        {
          "text": "set host-charset UTF-8",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "run import test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}\\build\\debug\\bin\\${workspaceFolderBasename}.exe",
      "args": ["-i", "test", "new_ffxkr.iso"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}\\build\\debug\\bin",
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "${env:MSYS2_HOME}\\mingw64\\bin\\gdb.exe",
      "preLaunchTask": "Build",
      "internalConsoleOptions": "neverOpen",
      "setupCommands": [
        {
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "text": "set print unicode on",
          "ignoreFailures": true
        },
        {
          "text": "set target-charset UTF-8",
          "ignoreFailures": true
        },
        {
          "text": "set host-charset UTF-8",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
